	</servlet>
	<servlet-mapping>
		<servlet-name>registerAdminServlet</servlet-name>
		<url-pattern>/registerAdminServlet</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>
	<servlet>
		<servlet-name>selectProductServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.SelectProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>selectProductServlet</servlet-name>
		<url-pattern>/selectProductServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>productDetailServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.ProductDetailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>productDetailServlet</servlet-name>
		<url-pattern>/productDetailServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>changeProductServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.ChangeProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>changeProductServlet</servlet-name>
		<url-pattern>/changeProductServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>registerConfirmServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.RegisterConfirmServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>registerConfirmServlet</servlet-name>
		<url-pattern>/registerConfirmServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>changeConfirmServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.ChangeConfirmServlet</servlet-class>
	</servlet>
