		<servlet-class>jp.co.aforce.servlets.UserShoppingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserShoppingServlet</servlet-name>
		<url-pattern>/UserShopping</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ResultServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.ResultServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResultServlet</servlet-name>
		<url-pattern>/Result</url-pattern>
	</servlet-mapping>

</web-app>