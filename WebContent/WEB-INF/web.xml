		<servlet-name>changeConfirmServlet</servlet-name>
		<url-pattern>/changeConfirmServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ListProductServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.ListProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListProductServlet</servlet-name>
		<url-pattern>/ListProductServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteProductServlet</servlet-name>
		<servlet-class>jp.co.aforce.servlets.DeleteProductServlet</servlet-class>
	</servlet>
